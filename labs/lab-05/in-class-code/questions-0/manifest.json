{
    "name": "QUESTIONS (advanced)",
    "version": "1.0",
    "description": "Collecting Questions",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentscript.js"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": "tent.png",
      "default_popup": "popup/popup.html"
    }

  }
